{
  "name": "platno-json-to-yup",
  "version": "1.0.2",
  "description": "Build a Yup schema object from JSON configuration.",
  "source": "src/index.js",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "./node_modules/.bin/rimraf ./dist && ./node_modules/.bin/babel ./src --out-dir ./dist --ignore \"src/**/*.test.js\" --env-name prod",
    "lint": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "publish": "npm run build && npm publish",
    "test": "./node_modules/.bin/jest"
  },
  "author": "Petar Vudragovic <github@platno.rs>",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.21",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.3.0",
    "date-fns": "2.12.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-lodash": "7.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.3.8",
    "jest": "29.5.0",
    "rimraf": "3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "npm run lint && npm run test"
      ]
    }
  },
  "keywords": [
    "yup",
    "json",
    "schema",
    "model",
    "validate",
    "validation",
    "convert",
    "build",
    "builder"
  ]
}
